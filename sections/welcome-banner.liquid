{% schema %}

{
    "name": "Blog Banner",
    "settings": [
        {
            "type": "checkbox",
            "id": "show_icon",
            "label": "Show Icon",
            "default": true
        }, {
            "type": "text",
            "id": "blog_banner_title",
            "label": "Title"
        }, {
            "type": "textarea",
            "id": "blog_banner_subtitle",
            "label": "Subtitle"
        }, {
            "type": "select",
            "id": "spacing",
            "label": "Spacing",
            "options": [
                {
                    "value": "py-8 md:py-16",
                    "label": "sm"
                }, {
                    "value": "py-20",
                    "label": "md"
                }, {
                    "value": "py-32",
                    "label": "lg"
                }, {
                    "value": "py-40",
                    "label": "xl"
                }
            ]
        }
    ]
}

{% endschema %}